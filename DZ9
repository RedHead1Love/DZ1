using System;

class Program
{
    static void Main()
    {
        Console.Write("Введите сообщение: ");
        string message = Console.ReadLine();

        Console.Write("Введите количество повторов: ");
        int repeatCount = int.Parse(Console.ReadLine());

        for (int i = 0; i < repeatCount; i++)
        {
            Console.WriteLine(message);
        }
    }
}
