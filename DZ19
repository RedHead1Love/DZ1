using System;

class Program
{
    static void Main()
    {
        const int minDivisor = 10;
        const int maxDivisor = 26;
        const int minRange = 50;
        const int maxRange = 150;

        Random random = new Random();

        int divisor = random.Next(minDivisor, maxDivisor);

        Console.WriteLine($"Делитель: {divisor}");

        int count = 0;
        int stepsToNextMultiple = 0; 

        for (int i = minRange; i <= maxRange; i++)
        {
            stepsToNextMultiple++;

            if (stepsToNextMultiple == divisor)
            {
                count++;
                stepsToNextMultiple = 0;
            }
        }

        Console.WriteLine($"Количество кратных чисел: {count}");
    }
}
