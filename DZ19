using System;

class Program
{
    static void Main()
    {
        int minimumRandomNumber = 10;
        int maximumRandomNumber = 26;

        int minimumMultipleRandom = 50;
        int maximumMultipleRandom = 150;

        Random random = new Random();

        int randomNumber = random.Next(minimumRandomNumber, maximumRandomNumber); 

        int i = 0;

        for (int multiple = randomNumber; multiple <= maximumMultipleRandom; multiple += randomNumber)
        {
            if (multiple >= minimumMultipleRandom)
            {
                i++;
            }
        }

        Console.WriteLine($"Рандомное число = {randomNumber}");
        Console.WriteLine($"Количество кратных чисел от {minimumMultipleRandom} до {maximumMultipleRandom}: {i}");
    }
}
