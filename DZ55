using System;
using System.Collections.Generic;
using System.Linq;

class Program
{
    static void Main()
    {
        List<Stew> stews = InitializeStews();
        DisplayExpiredStews(stews);
    }

    private class Stew
    {
        public Stew(string name, int productionYear, int expirationYears)
        {
            Name = name;
            ProductionYear = productionYear;
            ExpirationYears = expirationYears;
        }

        public string Name { get; }
        public int ProductionYear { get; }
        public int ExpirationYears { get; }

        public bool IsExpired(int currentYear) => currentYear > ProductionYear + ExpirationYears;

        public override string ToString() => $"{Name} (Год производства: {ProductionYear}, Срок годности: {ExpirationYears} лет)";
    }

    static List<Stew> InitializeStews()
    {
        int yearFirstProduction = 2018;
        int yearSecondProductiont = 2020;
        int yearThirdProduction = 2022;
        int yearFourthProduction = 2015;
        int yearFifthProduction = 2019;
        int yearSixthProduction = 2021;
        int yearSeventhProduction = 2023;
        int yearEighthProduction = 2017;
        int yearNinthProduction = 2020;
        int yearTenthProduction = 2022;

        int expirationDateFirstProduct = 5;
        int expirationDateSecondProduct = 4;
        int expirationDateThirdProduct = 3;
        int expirationDateFourthProduct = 7;
        int expirationDateFifthProduct = 6;
        int expirationDateSixthProduct = 5;
        int expirationDateSeventhProduct = 10;
        int expirationDateEighthProduct = 3;
        int expirationDateNinthProduct = 5;
        int expirationDateTenthProduct = 8;

        int currentYear = DateTime.Now.Year;

        return new List<Stew>
        {
            new Stew("Говяжья тушенка", yearFirstProduction, expirationDateFirstProduct),
            new Stew("Свиная тушенка", yearSecondProductiont, expirationDateSecondProduct),
            new Stew("Куриная тушенка", yearThirdProduction, expirationDateThirdProduct),
            new Stew("Оленина в собственном соку", yearFourthProduction, expirationDateFourthProduct),
            new Stew("Тушенка армейская", yearFifthProduction, expirationDateFifthProduct),
            new Stew("Тушенка домашняя", yearSixthProduction, expirationDateSixthProduct),
            new Stew("Тушенка охотничья", yearSeventhProduction, expirationDateSeventhProduct),
            new Stew("Тушенка деликатесная", yearEighthProduction, expirationDateEighthProduct),
            new Stew("Тушенка по-домашнему", yearNinthProduction, expirationDateNinthProduct),
            new Stew("Тушенка премиум", yearTenthProduction, expirationDateTenthProduct)
        };
    }

    static void DisplayExpiredStews(List<Stew> stews)
    {
        int currentYear = DateTime.Now.Year;

        var expiredStews = stews
            .Where(stew => stew.IsExpired(currentYear))
            .ToList();

        Console.WriteLine($"Просроченные банки тушенки на {currentYear} год:");
        Console.WriteLine(expiredStews.Any() ? string.Join("\n", expiredStews) : "Просроченных банок не найдено");
    }
}
